{
  "compilerOptions": {
    "module": "NodeNext",                         
    "moduleResolution": "NodeNext",
    "target": "ES2020",                         
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "src",
    "noImplicitAny": true,
    "sourceMap": true
  },
  "include": [ "src/**/*" ],
}
